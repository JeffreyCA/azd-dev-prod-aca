{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "envType": {
        "value": "${AZURE_ENV_TYPE=dev}"
      },
      "devProdPcAcaExists": {
        "value": "${SERVICE_DEV_PROD_PC_ACA_RESOURCE_EXISTS=false}"
      },
      "customDomain": {
        "value": "${CUSTOM_DOMAIN_NAME}"
      },
      "managedCertId": {
        "value": "${MANAGED_CERT_ID}"
      }
    }
}
